{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": false
    },
    "target": "es2020",
    "loose": false,
    "minify": {
      "compress": false,
      "mangle": false
    }
  },
  "module": {
    "type": "commonjs",
    "ignoreDynamic": true,
    "strict": true
  },
  "minify": false,
  "sourceMaps": "inline",
  "exclude": [".test.ts"]
}
